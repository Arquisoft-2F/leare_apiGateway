#este es archivo de graphql de definiciones NO LE CAMBIEN EL NOMBRE  
type Query {
    users: [Users]
    userById(id: ID!): Users
}

type Users{
    id: ID
    nickname: String
    name: String
    lastname: String
    picture_id: String
    nationality: String
    email: String
    web_site: String
    biography: String
    twitter_link: String
    linkedin_link: String
    facebook_link: String
    created_at: String
    updates_at: String
    # ya se imaginan que va
}
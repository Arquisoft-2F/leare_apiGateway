type Chat {
    user_id: ID!
    user_nickname: String!
    user_is_admin: Boolean!
    user_last_read: String
    chat: ChatData
}

type ChatData {
    chat_id: ID!
    chat_name: String!
    picture_id: String
    created_at: String
    last_message: Message
}

type Message {
    sender_id: ID!
    sender_nickname: String!
    content: String!
    created_at: String
    updated_at: String
}

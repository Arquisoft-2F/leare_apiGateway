type Highlight{
    name: [String]
    lastname: [String]
    nickname: [String]
    description: [String]
}

type Post{
    id: ID!
    name: String!
    lastname: String
    nickname: String
    description: String
    picture: String
    type: String
}

type ResponsePost {
    highlight: Highlight
    post: Post
    error: String
}
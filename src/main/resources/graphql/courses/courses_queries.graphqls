extend type Query {
    categories: [Category]
    categoryById(id: ID!): Category

    coursesByCategory(categories: [String]!): [Course]
    listCourses(page: Int!): [Course]
    courseById(id: ID!): Course

    moduleById(id: ID!): CourseModule
    courseModules(course_id: ID!, page: Int!): [CourseModule]
}